  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorksDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProductCategory">
            <Value>=Parameters!ProductCategory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     d.CalendarYear AS Year, d.MonthNumberOfYear AS MonthNo, d.EnglishMonthName AS Month, c.EnglishProductCategoryName AS ProductCategory, 
                      s.EnglishProductSubcategoryName AS ProductSubcategory, p.EnglishProductName AS Product, i.SalesOrderNumber, i.OrderDate, i.SalesAmount
FROM         DimProductCategory AS c INNER JOIN
                      DimProductSubcategory AS s ON s.ProductCategoryKey = c.ProductCategoryKey INNER JOIN
                      DimProduct AS p ON p.ProductSubcategoryKey = s.ProductSubcategoryKey INNER JOIN
                      FactInternetSales AS i ON i.ProductKey = p.ProductKey INNER JOIN
                      DimDate AS d ON i.OrderDateKey = d.DateKey
WHERE c.EnglishProductCategoryName = @ProductCategory
ORDER BY Year, MonthNo, ProductCategory, ProductSubcategory, Product, i.SalesOrderNumber
</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="MonthNo">
          <DataField>MonthNo</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductCategory">
          <DataField>ProductCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductSubcategory">
          <DataField>ProductSubcategory</DataField>
